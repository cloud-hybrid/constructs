{
    "name": "cloud-iac",
    "private": false,
    "version": "0.7.215",
    "type": "module",
    "types": "index.d.ts",
    "main": "index.js",
    "description": "Cloud Construct Package + Utilities",
    "license": "BSD-2-Clause",
    "author": "Jacob B. Sanders",
    "scripts": {
        "get": "cdktf get",
        "cdktf": "cdktf",
        "build": "tsc --pretty",
        "ci-cd": "cd ci-cd && npm run build && cd -",
        "compile": "tsc --watch",
        "prepublishOnly": "npm run test",
        "upload": "npm publish --access public",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "rm-declarations": "find . -name '*.d.ts' -type f -delete",
        "rm-declaration-maps": "find . -name '*.d.ts.map' -type f -delete",
        "rm-maps": "find . -name '*.js.map' -type f -delete",
        "rm-compilations": "find . -name '*.js' -type f -delete",
        "rm-modules": "rm -rf node_modules",
        "test": "npm run build && node --experimental-vm-modules $(command -v jest) --config jest.config.js --injectGlobals",
        "clean": "npm run rm-declarations && npm run rm-declaration-maps && npm run rm-maps && npm run rm-compilations && npm run rm-modules"
    },
    "dependencies": {
        "@cdktf/provider-aws": "5.0.40",
        "@cdktf/provider-docker": "^0.6.30",
        "@cdktf/provider-null": "^0.5.31",
        "@cdktf/provider-random": "^0.1.36",
        "@cloud-technology/parameter": "^0.7.139",
        "@cloud-technology/secrets-manager": "^0.6.141",
        "@cloud-technology/secrets-manager-client": "^0.4.38",
        "@nodelib/fs.scandir": "^2.1.5",
        "@nodelib/fs.walk": "^1.2.8",
        "@swc/core": "^1.2.148",
        "@swc/helpers": "^0.3.6",
        "cdktf": "^0.9.4",
        "cdktf-cli": "0.9.4",
        "chalk": "^5.0.0",
        "constructs": "^10.0.81",
        "ts-node": "^10.7.0",
        "tslib": "^2.3.1",
        "glob": "^7.2.0",
        "inquirer": "^8.2.0",
        "mime-types": "^2.1.34",
        "open": "^8.4.0",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/inquirer": "^8.2.0",
        "@types/jest": "^27.4.1",
        "@types/mime-types": "^2.1.1",
        "@types/node": "^17.0.19",
        "@types/uuid": "^8.3.4",
        "@jest/globals": "^27.5.1",
        "jest": "^27.5.1",
        "ts-jest": "^27.1.3",
        "typescript": "^4.6.2"
    },
    "bin": {
        "cloud-iac": "ci-cd/index.js"
    }
}
